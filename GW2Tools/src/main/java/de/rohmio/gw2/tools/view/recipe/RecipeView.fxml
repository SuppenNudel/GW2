<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<fx:root alignment="TOP_CENTER" type="VBox" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="lbl_outputName" text="Label" />
      <GridPane>
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Source" />
            <Label text="Type" GridPane.rowIndex="1" />
            <Label text="Output qty." GridPane.rowIndex="2" />
            <Label text="Discipline" GridPane.rowIndex="3" />
            <Label text="Req. rating" GridPane.rowIndex="4" />
            <VBox fx:id="vbox_discipline" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label fx:id="lbl_source" text="Label" GridPane.columnIndex="1" />
            <Label fx:id="lbl_type" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label fx:id="lbl_outputQty" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label fx:id="lbl_reqRating" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="4" />
         </children>
      </GridPane>
      <Label text="Ingredients" />
      <VBox fx:id="vbox_ingredients" />
   </children>
</fx:root>
